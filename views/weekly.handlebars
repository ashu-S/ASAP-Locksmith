                </thead>
                <tbody>
                {{#each Job}}
                <tr>
                    <td class="Monthlytd">{{this.dataValues.TechnicianId}}</td>
                    <td class="Monthlytd">{{this.dataValues.client_name}}</td>
                    <td class="Monthlytd">{{this.dataValues.createdAt}}</td> 
                    <td class="Monthlytd">{{this.dataValues.services}}</td>
                    <td class="Monthlytd">{{this.dataValues.Technician.location}}</td>
                    <td class="Monthlytd">{{this.dataValues.Technician.current_job}}</td>
                    <td class="Monthlytd">{{this.dataValues.job_status}}</td>
                    
                     <!-- <td>{{ this.subject }}</td> -->
                </tr>
                {{/each}}
                </tbody> 
            </table>
        </div>

