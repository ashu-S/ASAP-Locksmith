<div class="body-webpage">
	<div class="jumbotron services-jumbotron">
			<div class="container">
				<h2 class="service-header">Welcome</h2>
		      <!-- START THE FEATURETTES -->
		      <hr class="featurette-divider">
		      <!-- Collect the nav links, forms, and other content for toggling -->
		      <div class="collapse navbar-collapse" id="navbar-collapse">
		       
						
		      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>
<!--End Nav-->

		      <div class="row featurette">
		      {{description}}
		        <table class="table table-striped">
                <thead>
                <tr>
                	<th>Technician Id</th>
                    <th>Client Name</th>
                    <th>Description</th>
                    <th>Location</th>
                    <th>Current Job</th>
                    <th>Job Status</th>
                    <!-- <th>Subject</th> -->
                </tr>
                </thead>
                <tbody>
                {{#each Job}}
                <tr>
                	<td>{{this.dataValues.Technician.TechnicianId}}</td>
                	<td>{{this.dataValues.client_name}}</td>
                    <td>{{this.dataValues.description}}</td>
                    <td>{{this.dataValues.location}}</td>
                    <td>{{this.dataValues.Technician.current_job}}</td>
                    <td>{{this.dataValues.Technician.job_status}}</td>
                    
                     <!-- <td>{{ this.subject }}</td> -->
                </tr>
                {{/each}}
                </tbody> 
            </table>


		      </div>

		      <hr class="featurette-divider">


	</div> <!-- END SERVICES JUMBOTRON
</div> <!-- End WEBPAGE BODY -->
