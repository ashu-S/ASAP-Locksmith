<div class="container">
			
         <div class="row featurette view-row">
            <div class="panel panel-primary filterable">
        <div class="panel-heading">
          <h3 class="panel-title view-panel-title">All Jobs</h3>
            <div class="pull-right">
              
            </div>
        </div>
              {{description}}
                <table class="table ">
                <thead>
                <tr>
                    <th>Technician Id</th>
                    <th>Client Name</th>
                    <th>Description</th>
                    <th>Location</th>
                    <th>Current Job</th>
                    <th>Job Status</th>
                    <!-- <th>Subject</th> -->
                </tr>
                </thead>
                <tbody>
                {{#each Job}}
                <tr>
                    <td>{{this.dataValues.TechnicianId}}</td>
                    <td>{{this.dataValues.client_name}}</td>
                    <td>{{this.dataValues.services}}</td>
                    <td>{{this.dataValues.Technician.location}}</td>
                    <td>{{this.dataValues.Technician.current_job}}</td>
                    <td>{{this.dataValues.Technician.job_status}}</td>
                    
                     <!-- <td>{{ this.subject }}</td> -->
                </tr>
                {{/each}}
                </tbody> 
            </table>


              </div>

              <hr class="featurette-divider">


    </div> <!-- END SERVICES JUMBOTRON -->

		     
